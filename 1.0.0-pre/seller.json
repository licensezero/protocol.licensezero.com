{
  "$schema": "http://json-schema.org/schema#",
  "$id": "https://protocol.licensezero.com/1.0.0-pre/seller.json",
  "type": "object",
  "required": [
    "email",
    "jurisdiction",
    "name",
    "website"
  ],
  "properties": {
    "email": {
      "type": "string",
      "format": "email"
    },
    "jurisdiction": {
      "$ref": "jurisdiction.json"
    },
    "name": {
      "type": "string",
      "minLength": 3
    },
    "website": {
      "$ref": "url.json"
    }
  }
}
