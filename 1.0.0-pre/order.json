{
  "$id": "https://schemas.licensezero.com/1.0.0-pre/order.json",
  "type": "object",
  "required": [
    "offerIDs",
    "licensee"
  ],
  "properties": {
    "offerIDs": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      }
    },
    "licensee": {
      "type": "object",
      "required": [
        "email",
        "jurisdiction",
        "name"
      ],
      "additionalProperties": true,
      "properties": {
        "email": {
          "type": "string",
          "format": "email"
        },
        "jurisdiction": {
          "$ref": "https://example.com/1.0.0-pre/iso31662.json"
        },
        "name": {
          "type": "string",
          "minLength": 3
        }
      }
    }
  }
}
